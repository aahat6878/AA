ICICI:



package pageobjectofICICI;



import org.openqa.selenium.By;

import org.openqa.selenium.JavascriptExecutor;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.FindBy;

import org.openqa.selenium.support.ui.Select;

import org.testng.annotations.AfterMethod;

import org.testng.annotations.BeforeMethod;

import org.testng.annotations.Test;



import driversetup.driversetup;



public class icici extends driversetup {



 WebDriver driver;

 @FindBy(xpath="//*[@id='tenureDay']")

 WebElement days;





 @BeforeMethod

 public void before()

 {

 driver = chromesetup();

 driver.manage().window().maximize();

 }



 @Test

 public void f() throws InterruptedException{



 Thread.sleep(2000);

 //JavascriptExecutor js = (JavascriptExecutor)driver;

 //js.executeScript("window.scrollBy(0,200);");

 driver.switchTo().frame("paymframe");

 Select fdt = new Select(driver.findElement(By.xpath("//*[@id='ddlTypeOfFixedDeposit']")));

 fdt.selectByValue("Quarterly Payout");

 driver.findElement(By.xpath("//*[@id='loanAmount']")).clear();

 driver.findElement(By.xpath("//*[@id='loanAmount']")).sendKeys("60000");

 String metval =	driver.findElement(By.xpath("//*[@id='spnMaturityValue']")).getText();

 String aiamt =	driver.findElement(By.xpath("//*[@id='spnAIAmount']")).getText();

 System.out.println("maturity value"+metval);

 System.out.println("aggreagate interest amount"+aiamt);

 driver.findElement(By.xpath("//*[@id='idDays']")).click();

 driver.findElement(By.xpath("//*[@id='tenureDay']")).clear();

 driver.findElement(By.xpath("//*[@id='tenureDay']")).sendKeys("1000");

 String metval1 =	driver.findElement(By.xpath("//*[@id='spnMaturityValue']")).getText();

 String aiamt1 =	driver.findElement(By.xpath("//*[@id='spnAIAmount']")).getText();

 System.out.println("maturity value"+metval1);

 System.out.println("aggreagate interest amount"+aiamt1);

 Thread.sleep(2000);

 JavascriptExecutor js1 = (JavascriptExecutor)driver;

 js1.executeScript("window.scrollBy(0,-1000);","");

 Thread.sleep(2000);

 driver.switchTo().parentFrame();



 WebElement logo = driver.findElement(By.xpath("//*[@class='Logo']"));

 js1.executeScript("arguments[0].scrollintoView;",logo);

 Actions ac = new Actions(driver);

 ac.moveToElement(logo);

 ac.click().build().perform();







 }



 @AfterMethod

 public void after(){



 driver.quit();

 }



 

}



