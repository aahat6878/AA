GOINDIGO:

package goindigo;



import java.io.IOException;

import java.util.List;



import org.openqa.selenium.By;

import org.openqa.selenium.JavascriptExecutor;

import org.openqa.selenium.Keys;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebDriver.Window;

import org.openqa.selenium.WebElement;

import org.testng.annotations.AfterMethod;

import org.testng.annotations.BeforeMethod;

import org.testng.annotations.Test;



import Goibibo.Excel;

import driversetup.driversetup;



public class goindigo extends driversetup {

 WebDriver driver;

	

 @BeforeMethod

  public void beforeMethod() {

  driver = chromesetup();

  driver.manage().window().maximize();

  }

  

 @Test

 public void f() throws IOException, InterruptedException {

  WebElement roundtrip = driver.findElement(By.xpath("//*[text()='Round Trip']"));

  JavascriptExecutor js = (JavascriptExecutor)driver;

  js.executeScript("arguments[0].click();",roundtrip);

  WebElement from = driver.findElement(By.xpath("//input[@placeholder='From' and @name='or-src']"));

  from.click();

  //from.clear();

  Thread.sleep(2000);

  String fr = Excel.getcellvalue("Sheet1",0,0);

  from.sendKeys(fr);

  //js.executeScript("arguments[0].value = '"+fr+"';", from);

  Thread.sleep(2000);

  from.sendKeys(Keys.ENTER);

  

  WebElement dest = driver.findElement(By.xpath("//input[@placeholder='To' and @name='or-dest']"));

  //dest.click();

  String dt = Excel.getcellvalue("Sheet1",0,1);

  dest.sendKeys(dt);

  //js.executeScript("arguments[0].value = '"+dt+"';", dest);

  Thread.sleep(5000);

  dest.sendKeys(Keys.ENTER);

  Thread.sleep(2000);

  driver.findElement(By.xpath("//input[@name='or-depart' and @data-parsley-required='true']"));

  

  js.executeScript("window.scrollBy(0,200)");

  Thread.sleep(2000);

  driver.findElement(By.xpath("//*[@data-month='11']//a[text()='18']")).click();

  

  driver.findElement(By.xpath("//input[@name='or-return' and @data-parsley-flightreturndate='true']")).click();

  driver.findElement(By.xpath("//*[@data-month='11']//a[text()='19']")).click();

  driver.findElement(By.name("passenger")).click();

  List<WebElement> list = driver.findElements(By.xpath("//*[@class='icon-plus']"));

  list.get(0).click();

  driver.findElement(By.xpath("//*[@class='passenger-done-blck']//*[text()='Done']")).click();

  driver.findElement(By.xpath("//*[@type='submit']//*[text()='Search Flight']")).click();

  

  

  

  

  

 }

  

 @AfterMethod

 public void afterMethod() {

  driver.quit();

 }

}

